{
  "name": "crescent",
  "version": "0.0.1",
  "description": "JavaScript images Binarization/Comparsion",
  "main": "lib/crescent.js",
  "scripts": {
    "build": "babel src --out-file lib/crescent.js",
    "start": "npm run build -- --watch",
    "release": "npm run build && npm publish",
    "ci": "karma start --single-run",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otiai10/crescentjs.git"
  },
  "keywords": [
    "JavaScript",
    "Image",
    "Comparison"
  ],
  "author": "otiai10",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/otiai10/crescentjs/issues"
  },
  "homepage": "https://github.com/otiai10/crescentjs#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserify": "^5.0.5",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
